    "version": "0.2.0",
    "configurations": [
{
            "name": "Launch Chrome against Docker Node Debugger",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "address": "localhost",
            "restart": false,
            "sourceMaps": false,
            "localRoot": "${workspaceRoot}/",
            "remoteRoot": "/"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost on on port 4200",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}"
        }
       
    ]
}